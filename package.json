    "test": "ts-node test/test.ts"
  },
  "dependencies": {
    "@types/node": "^14.11.2",
    "axios": "^0.20.0",
    "bindings": "^1.5.0",
    "ghreleases": "^3.0.2",
    "github-from-package": "0.0.0",
    "node-addon-api": "^3.0.2",
    "tar-fs": "^2.1.0",
    "x11": "^2.3.0"
  },
  "devDependencies": {
    "cmake-js": "^6.1.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3"
  }
}
