{
  "name": "libobs-node",
  "version": "1.0.2",
  "repository": "https://github.com/Mengli619/libobs-node.git",
  "main": "dist/index.js",
  "scripts": {
    "build": "rm -rf build/Debug && cmake-js configure --CDCMAKE_BUILD_TYPE=Debug --CDOBS_STUDIO_DIR=${OBS_STUDIO_DIR} && cmake --build build --config Debug",
